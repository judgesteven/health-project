{
  "version": 2,
  "name": "health-gamification-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run web:build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null
}
